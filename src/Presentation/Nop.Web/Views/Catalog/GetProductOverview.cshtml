@{
    Layout = null;
}

@model ProductOverview

<div id="float-container-product-overview">
    <div class="float-child-product-overview">
        <h3>Title: </h3>
        <h3>Price: </h3>
        <h3>Description: </h3>
        <h3>Images: </h3>
    </div>
    <div class="float-child-product-overview">
        <p> @Model.Name </p>
        <p> @Model.Price</p>
        <p> @Model.ShortDescription </p>
        @foreach (var picture in Model.UrlsString)
        {
            <img id="productImagePreview" alt="" src="@picture?" width="100" height="100"/>
        }
    </div>
</div>